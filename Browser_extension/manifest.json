{
	"manifest_version": 3,
	"name":"FileSec",
	"version":"1.0.0",
	"description": "PDF and word file security system and sanitizer",
	"host_permissions": ["<all_urls>"],
    "permissions": [
		"downloads",
		"downloads.open", 
		"downloads.shelf", 
		"storage"
	],
	"action":{
		"default_popup":"index.html"
	},
	"background": {
		"service_worker": "/backend.js"
	  }
}
